<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
    <sitecore>
        <pipelines>
            <renderContentEditor>
                <processor patch:before="*[1]" type="Knights.Feature.KeywordFieldHelper.Processors.InjectScripts, Knights.Feature.KeywordFieldHelper" />
            </renderContentEditor>
            <initialize>
                <processor type="Knights.Feature.KeywordFieldHelper.Processors.RegisterApiRoutes, Knights.Feature.KeywordFieldHelper" />
            </initialize>
        </pipelines>
        <settings>
            <setting name="CustomContentEditorJavascript" value="/sitecore modules/KeywordFieldHelper/keywordfieldhelper.js"/>
        </settings>
    </sitecore>
</configuration>
